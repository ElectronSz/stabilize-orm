{
  "name": "stabilize-orm",
  "version": "1.3.9",
  "description": "A lightweight, type-safe ORM for Bun.js with support for SQLite, MySQL, PostgreSQL, and Redis caching",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "bun build src/index.ts --outdir dist --target bun --minify",
    "prepublishOnly": "bun run build && bun run test",
    "format": "bunx prettier --write ."
  },
  "keywords": [
    "orm",
    "bun",
    "sqlite",
    "mysql",
    "postgresql",
    "redis",
    "typescript",
    "database"
  ],
  "author": "ElectronSz <lwazicd@icloud.com>",
  "license": "MIT",
  "dependencies": {
    "@types/pg": "^8.15.5",
    "@types/uuid": "^11.0.0",
    "commander": "^12.1.0",
    "figlet": "^1.9.3",
    "glob": "^11.0.3",
    "ioredis": "^5.8.1",
    "mysql2": "^3.15.2",
    "pg": "^8.16.3",
    "reflect-metadata": "^0.2.2",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.46.1",
    "@typescript-eslint/parser": "^8.46.1",
    "@vitest/coverage-v8": "^2.1.9",
    "eslint": "^9.38.0",
    "prettier": "^3.6.2",
    "typescript": "^5.9.3",
    "vitest": "^2.1.9"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ElectronSz/stabilize-orm.git"
  },
  "bugs": {
    "url": "https://github.com/ElectronSz/stabilize-orm/issues"
  },
  "homepage": "https://github.com/ElectronSz/stabilize-orm#readme",
  "engines": {
    "bun": ">=1.0.0"
  },
  "peerDependencies": {
    "bun": "^1.3.0"
  }
}
